# Oracle 연결 정보
DB_HOST=your_db_host
DB_PORT=1521
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_SERVICE=your_db_service

# CORS 설정
CORS_ORIGINS=http://localhost:3000

# 개발 환경 설정
FLASK_ENV=development
ALLOW_ANONYMOUS_USAGE=true
ANONYMOUS_USER_ID=your_anonymous_user_id

# OCR 설정
OCR_UPLOAD_FOLDER=uploads
OCR_MAX_CONTENT_LENGTH=10485760

# JWT 설정
JWT_SECRET_KEY=your_jwt_secret_key
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=60

# Gemini API 설정
# Google AI Studio(https://makersuite.google.com/app/apikey)에서 API 키 발급
# 무료 API 키의 경우 분당 60회 요청 제한이 있으며, API 키가 없거나 할당량 초과 시 기본 OCR로 처리됨
GEMINI_API_KEY=your_gemini_api_key

# 이메일 설정
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your_email@gmail.com
EMAIL_PASSWORD=your_app_password
EMAIL_FROM=your_email@gmail.com 